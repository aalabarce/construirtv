<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Titulos" table="titulos">
    <indexes>
      <index name="genero_id" columns="genero_id,serie_id"/>
      <index name="serie_id" columns="serie_id"/>
      <index name="IDX_90A706DFBCE7B795" columns="genero_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="100" nullable="false"/>
    <field name="nroCapitulo" type="integer" column="nro_capitulo" nullable="false"/>
    <field name="duracion" type="integer" column="duracion" nullable="false"/>
    <field name="temporada" type="integer" column="temporada" nullable="false"/>
    <field name="subtitulado" type="boolean" column="subtitulado" nullable="false"/>
    <field name="sinopsis" type="string" column="sinopsis" length="4000" nullable="false"/>
    <field name="urlVimeo" type="string" column="url_vimeo" length="100" nullable="false"/>
    <field name="destacado" type="boolean" column="destacado" nullable="false"/>
    <many-to-one field="genero" target-entity="Generos">
      <join-columns>
        <join-column name="genero_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="serie" target-entity="Series">
      <join-columns>
        <join-column name="serie_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
